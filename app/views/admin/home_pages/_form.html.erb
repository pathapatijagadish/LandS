<div class='bodyCnt'>
  <div class='wraper'>
    <div class='' id='inner-main'>
      <div class='subNavCnt'>
        <h1>New Sequence</h1>
        <div class='moreLinks'>
          <ul>
            <li>
              <%= link_to 'Back', admin_home_pages_path %>
            </li>
          </ul>
        </div>
        <div class='clear'></div>
      </div>
      <div class='wraper'>
        <%= nested_form_for([:admin,@home_page]) do |f| %>
          <div class='sectionCnt'>
            <h2>
            Home Page
            </h2>
            <div class='formLayout'>
              <div class='row'>
                <%= f.label :title %><br>
                <div class='inputCnt'>
                  <%= f.text_field :title %>
                </div>
                <div class='clear'></div>
              </div>

              <div class='row'>
                <%= f.label :description %><br>
                <div class='inputCnt'>
                  <%= f.text_area :description %>
                  </textarea>
                </div>
                <div class='clear'></div>
              </div>

              <div class='row'>
                <%= f.label :sequence %><br>
                <div class='inputCnt'>
                  <%= f.number_field :sequence %><br>
                </div>
                <div class='clear'></div>
              </div>

              <div class='row'>
                <%= f.label :status %><br>
                <div class='inputCnt'>
                  <%= f.check_box :status %>
                </div>
                <div class='clear'></div>
              </div>

              <div class='row'>
                <%= f.label :button %><br>
                <div class='inputCnt'>
                  <%= f.text_field :button %>
                </div>
                <div class='clear'></div>
              </div>

              <div class='row'>
                <%= f.label :link %><br>
                <div class='inputCnt'>
                  <%= f.text_field :link %>
                </div>
                <p style='font: 12px arial;'>Please add link '/about_us#blackboxlink' If you want to guide to 'Hazard Free Environment' on click</p>
                <div class='clear'></div>
              </div>

              <div class='row'>
              <div>
                <%#= image_tag(@service.photo_url) if f.photo? %>
                <%= f.label :image %>
                <%= f.file_field :photo %>
                <%= f.hidden_field :photo_cache %>
              </div>
                <div class='clear'></div>
              </div>

              
              <div class='actions'>
                <%= f.submit %>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div class='clear'></div>
  <div class='clear'></div>
</div>



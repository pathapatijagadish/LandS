<div class='bodyCnt'>
  <div class='wraper'>
    <div class='' id='inner-main'>
      <div class='subNavCnt'>
        <h1>Projects</h1>
        <div class='moreLinks'>
          <ul>
            <li>
              <%= link_to 'New Service', new_admin_service_path %>
            </li>
          </ul>
        </div>
        <div class='clear'></div>
      </div>
      <table class='cmsTable'>
        <tr>
          <th>Sequence</th>
          <th>Title</th>
          <th>Description</th>
          <th>Image</th>
          <th></th>
          <th></th>
        </tr>
        <% @services.each do |service| %>
          <tr>
            <td><%= service.sequence %></td>
            <td><%= service.title %></td>
            <td><%= service.description %></td>
            <td><%= image_tag(service.photo_url,size: "80x46") %></td>
            <td><%= link_to 'Edit', edit_admin_service_path(service) %></td>
            <td><%= link_to 'Destroy', admin_service_path(service), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </table>
      <br>
      <br>
    </div>
  </div>
  <div class='clear'></div>
  <div class='clear'></div>
</div>

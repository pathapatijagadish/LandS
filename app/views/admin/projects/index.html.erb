<div class='bodyCnt'>
  <div class='wraper'>
    <div class='' id='inner-main'>
      <div class='subNavCnt'>
        <h1>Projects</h1>
        <div class='moreLinks'>
          <ul>
            <li>
              <%= link_to 'New Service', new_admin_project_path %>
            </li>
          </ul>
        </div>
        <div class='clear'></div>
      </div>
      <table class='cmsTable'>
        <tr>
          <th>Sequence</th>
          <th>Title</th>
          <th>Description</th>
          <th>Image</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
        <% @projects.each do |project| %>
          <tr>
            <td><%= project.sequence %></td>
            <td><%= project.title %></td>
            <td><%= project.description %></td>
            <td><%= image_tag(project.photo_url,size: "80x46") %></td>     
            <td><%= link_to 'Show', admin_project_path(project) %></td>
            <td><%= link_to 'Edit', edit_admin_project_path(project) %></td>
            <td><%= link_to 'Destroy', admin_project_path(project), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </table>
      <br>
      <br>
    </div>
  </div>
  <div class='clear'></div>
  <div class='clear'></div>
</div>

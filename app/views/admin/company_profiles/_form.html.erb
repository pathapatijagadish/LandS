<div class='bodyCnt'>
  <div class='wraper'>
    <div class='' id='inner-main'>
      <div class='subNavCnt'>
        <h1>New Sequence</h1>
        <div class='moreLinks'>
          <ul>
            <li>
              <%= link_to 'Back', admin_company_profiles_path %>
            </li>
          </ul>
        </div>
        <div class='clear'></div>
      </div>
      <div class='wraper'>
        <%= form_for([:admin,@company_profile],:html=>{:multipart =>true} ) do |f| %>
          <div class='sectionCnt'>
            <h2>
            Company Profile
            </h2>
            <div class='formLayout'>

              <div class='row'>
                <%= f.label :pdf %><br>
                <%= f.file_field :photo %>
                <%= f.hidden_field :photo_cache %>
                <div class='clear'></div>
              </div>

              <div class='actions'>
                <%= f.submit %>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div class='clear'></div>
  <div class='clear'></div>
</div>